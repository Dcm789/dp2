<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- 通用背景色 -->
    <SolidColorBrush x:Key="normalBackground" Color="Black"/>
    <SolidColorBrush x:Key="normalDialogBackground" Color="#333333"/>

    <SolidColorBrush x:Key="redBackground" Color="DarkRed"/>
    <SolidColorBrush x:Key="greenBackground" Color="DarkGreen"/>
    <SolidColorBrush x:Key="blueBackground" Color="DarkBlue"/>
    <SolidColorBrush x:Key="yellowBackground" Color="DarkOrange"/>
    <SolidColorBrush x:Key="grayBackground" Color="DarkGray"/>

    <SolidColorBrush x:Key="buttonBackground" Color="#FF90CAF9"/>
    <SolidColorBrush x:Key="buttonForeground" Color="#444444"/>

    <!-- 通用前景色 -->
    <SolidColorBrush x:Key="normalForeground" Color="White"/>
    <SolidColorBrush x:Key="softForeground" Color="#bfbfbf"/>


</ResourceDictionary>