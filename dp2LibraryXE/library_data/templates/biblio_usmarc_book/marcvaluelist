      <Label xml:lang="zh">汉语</Label>
    </Item>
    <Item>
      <Value>f</Value>
      <Label xml:lang="en">Arabic</Label>
      <Label xml:lang="zh">阿拉伯语</Label>
    </Item>
    <Item>
      <Value>g</Value>
      <Label xml:lang="en">Greek</Label>
      <Label xml:lang="zh">希腊语</Label>
    </Item>
    <Item>
      <Value>h</Value>
      <Label xml:lang="en">Hebrew</Label>
      <Label xml:lang="zh">希伯来语</Label>
    </Item>
